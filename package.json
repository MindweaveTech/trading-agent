{
  "name": "trading-agent",
  "version": "0.1.0",
  "private": true,
  "description": "AI Trading Agent Pilot - Zerodha MCP Integration for Mindweave Technologies",
  "scripts": {
    "dev": "PORT=3456 next dev",
    "dev:verbose": "PORT=3456 NODE_ENV=development DEBUG=* next dev",
    "build": "next build",
    "start": "next start -p 3456",
    "lint": "next lint",
    "deploy": "vercel --prod",
    "type-check": "tsc --noEmit",
    "logs:view": "tail -f logs/combined-$(date +%Y-%m-%d).log",
    "logs:errors": "tail -f logs/error-$(date +%Y-%m-%d).log",
    "server:start": "./scripts/start_server.sh",
    "server:stop": "./scripts/stop_server.sh",
    "server:restart": "./scripts/restart_server.sh"
  },
  "dependencies": {
    "@tensorflow/tfjs": "^4.20.0",
    "@tensorflow/tfjs-node": "^4.20.0",
    "@vercel/kv": "^1.0.0",
    "@vercel/postgres": "^0.9.0",
    "date-fns": "^3.6.0",
    "next": "^14.2.0",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "swr": "^2.2.0",
    "winston": "^3.18.3",
    "winston-daily-rotate-file": "^5.0.0",
    "ws": "^8.18.0",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@types/node": "^20.14.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@types/winston": "^2.4.4",
    "@types/ws": "^8.5.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.18",
    "typescript": "^5.5.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
